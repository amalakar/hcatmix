<database>
    <tables>
        <table>
            <namePrefix>hcatmix_600_paritions</namePrefix>
            <dbName>default</dbName>
            <columns>
                <column>
                    <name>user</name>
                    <type>string</type>
                    <avgLength>10</avgLength>
                    <distribution>uniform</distribution>
                    <cardinality>10000</cardinality>
                    <percentageNull>10</percentageNull>
                </column>
                <column>
                    <name>timespent</name>
                    <type>int</type>
                    <distribution>uniform</distribution>
                    <cardinality>100000</cardinality>
                    <percentageNull>10</percentageNull>
                </column>
                <column>
                    <name>query_term</name>
                    <type>string</type>
                    <avgLength>10</avgLength>
                    <distribution>uniform</distribution>
                    <cardinality>10000</cardinality>
                    <percentageNull>10</percentageNull>
                </column>
                <column>
                    <name>ip_addr</name>
                    <type>int</type>
                    <distribution>uniform</distribution>
                    <cardinality>100000</cardinality>
                    <percentageNull>10</percentageNull>
                </column>

                <column>
                    <name>estimated_revenue</name>
                    <type>int</type>
                    <distribution>uniform</distribution>
                    <cardinality>10000000</cardinality>
                    <percentageNull>10</percentageNull>
                </column>
                <column>
                    <name>page_info</name>
                    <type>string</type>
                    <avgLength>10</avgLength>
                    <distribution>uniform</distribution>
                    <cardinality>10000</cardinality>
                    <percentageNull>10</percentageNull>
                </column>
            </columns>
            <partitions>
                <partition>
                    <name>action</name>
                    <type>int</type>
                    <distribution>uniform</distribution>
                    <cardinality>2</cardinality>
                </partition>
                <partition>
                    <name>sourceid</name>
                    <type>int</type>
                    <distribution>uniform</distribution>
                    <cardinality>10</cardinality>
                </partition>
                <partition>
                    <name>testid</name>
                    <type>string</type>
                    <avgLength>5</avgLength>
                    <distribution>uniform</distribution>
                    <cardinality>30</cardinality>
                </partition>
            </partitions>
            <instances>
                <instance>
                    <size>2000000</size><!-- 210 MB -->
                    <count>1</count>
                </instance>
            </instances>
        </table>
    </tables>
</database>



